{
  "name": "roadmap",
  "version": "1.0.0",
  "description": "Roadmap application with Tailwind CSS",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "dev": "nodemon app.js",
    "build": "tailwindcss -i ./src/input.css -o ./dist/styles.css",
    "watch": "tailwindcss -i ./src/input.css -o ./dist/styles.css --watch",
    "build-css": "npm run build"
  },
  "dependencies": {
    "express": "^4.18.2",
    "ejs": "^3.1.9",
    "mysql2": "^3.6.5",
    "express-session": "^1.17.3",
    "passport": "^0.7.0",
    "passport-oauth2": "^1.7.0",
    "axios": "^1.6.2",
    "body-parser": "^1.20.2",
    "cookie-parser": "^1.4.6",
    "method-override": "^3.0.0",
    "dotenv": "^16.3.1",
    "express-rate-limit": "^7.1.5"
  },
  "devDependencies": {
    "tailwindcss": "^3.3.6",
    "postcss": "^8.4.31",
    "nodemon": "^3.0.2"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
