    "tts_model": "gpt-4o-mini-tts",
    "default_voice": "alloy",
    "available_voices": ["alloy", "ash", "ballad", "coral", "echo", "fable", "nova", "onyx", "sage", "shimmer"],
    "audio_settings": {
        "sample_rate": 24000,
        "format": "mp3",
